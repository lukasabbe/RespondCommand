{
  "schemaVersion": 1,
  "id": "respondmod",
  "version": "${version}",
  "name": "RespondMod",
  "description": "Add an /r command to your server. You can respond to the latest msg you got with /r",
  "authors": ["Lukasabbe"],
  "contact": {
    "repo": "https://github.com/lukas/RespondMod"
  },
  "license": "MIT",
  "icon": "assets/respondmod/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      "me.lukasabbe.respondmod.RespondMod"
    ]
  },
  "mixins": [
    "respondmod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=1.16.2 <=1.18.2"
  }
}
